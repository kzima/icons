    "node-sass": "^3.4.1",
    "optimist": "^0.6.1",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "vue-router": "^0.7.7",
    "vuestrap-docs": "^0.1.0",
    "vuestrap-theme-loader": "^0.1.2",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.12.1"
